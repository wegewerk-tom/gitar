{
    "name": "wegewerk/devops/GitAr",
    "type": "project",
    "description": "Archive and restore .git directories in sub-directories.",
    "license": "http://www.apache.org/licenses/LICENSE-2.0 Apache License, Version 2.0",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Thomas Stec ",
            "email": "initialinitialc@wegewerk.com"
        },
        {
            "name": "wegewerk GmbH",
            "homepage": "http://www.wegewerk.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "kzykhys/git": "dev-master",
        "theseer/directoryscanner": "1.3.*",
        "symfony/options-resolver": "2.3.*",
        "vanilla/garden-cli": "1.2.*",
        "seld/cli-prompt":"1.0.*"
    },
    "autoload": {
        "psr-4": {
            "wegewerk\\devops\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "vendor-dir": "vendor",
        "bin-dir": "bin"
    }
}